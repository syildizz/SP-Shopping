{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "globalize@1.7.0",
      "destination": "wwwroot/lib/globalize/"
    },
    {
      "library": "cldrjs@0.5.5",
      "destination": "wwwroot/lib/cldrjs/"
    },
    {
      "provider": "jsdelivr",
      "library": "jquery-validation-globalize@0.1.1",
      "destination": "wwwroot/lib/jquery-validation-globalize/"
    },
    {
      "library": "bootstrap-icons@1.11.3",
      "destination": "wwwroot/lib/bootstrap-icons/",
      "files": [
        "font/fonts/bootstrap-icons.woff",
        "font/fonts/bootstrap-icons.woff2",
        "font/bootstrap-icons.min.css"
      ]
    },
    {
      "provider": "unpkg",
      "library": "@microsoft/signalr@8.0.7",
      "destination": "wwwroot/lib/microsoft/signalr/",
      "files": [
        "dist/browser/signalr.js.map",
        "dist/browser/signalr.js",
        "dist/browser/signalr.min.js",
        "dist/browser/signalr.min.js.map"
      ]
    }
  ]
}